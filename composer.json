{
    "name": "yii-extensions/template",
    "type": "library",
    "description": "Template giyhub yii",
    "keywords": [
        "github",
        "template",
        "yii"
    ],
    "homepage": "https://github.com/yii-extensions/template",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "wilmer.arambula@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0"
    },
    "require-dev": {
        "codeception/codeception": "^3.0",
        "codeception/c3": "2.*",
        "dms/phpunit-arraysubset-asserts": "^0.1.0",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {"Yii\\Extensions\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Yii\\Extensions\\tests\\": "tests/"}
	},
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "defines": "config/defines.php",
            "common": "config/common.php",
            "params": [
                "config/params.php"
            ],
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
